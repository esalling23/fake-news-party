<section class="row" id="nextRound" style="margin-top:5%;">
    {{#if last_round}}
    
        {{>shared/typeform this}}
          
        <div id="results" class="col-lg-12">
            <div class="col-lg-4"></div>
            <div class="col-lg-4 game-message" >
                <span class="text col-lg-12" style="padding:10%">game over!<br />see main screen for results!</span>
            </div>
        </div>
    {{else}}
          
        <div class="col-lg-12">
            <div class="col-lg-4"></div>
            <div class="col-lg-4 game-message" >
                <span class="text col-lg-12" style="padding:10%">wait for the next round to start!</span>
            </div>
        </div>

    {{/if}}
</section>